<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wms.menu.model.dao.ReceiptProductLogMapper">
    <insert id="insertReceiptProduct" >
        insert into
            receipt_product (receipt_no,inbound_no, product_no, amount,cargo_space)
        values (#{receiptNo},#{inboundNo}, #{productNo}, #{amount},#{cargoSpace})

    </insert>

    <insert id="insertReceiptLog" useGeneratedKeys="true" keyProperty="receiptNo" >
        insert into receipt_log()
        values ()
    </insert>

</mapper>